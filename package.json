{
  "name": "trading-ui",
  "version": "1.0.1",
  "description": "This is an app to visualize the health of HK stock market",
  "main": "src/index.html",
  "scripts": {
    "start": "env-cmd -f .env.development parcel src/index.html",
    "start:prod": "env-cmd -f .env.production parcel src/index.html",
    "build": "env-cmd -f .env.production parcel build src/index.html",
    "watch": "env-cmd -f .env.development parcel watch src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Chengdong Zheng",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "dependencies": {
    "lightweight-charts": "^4.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "env-cmd": "^10.1.0",
    "parcel-bundler": "^1.12.4"
  }
}
